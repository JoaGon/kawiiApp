function n(n) {
  return n = n || Object.create(null), { on: function on(t, o) {
      (n[t] || (n[t] = [])).push(o);
    }, off: function off(t, o) {
      n[t] && n[t].splice(n[t].indexOf(o) >>> 0, 1);
    }, emit: function emit(t, o) {
      (n[t] || []).map(function (n) {
        n(o);
      }), (n["*"] || []).map(function (n) {
        n(t, o);
      });
    } };
}module.exports = n;